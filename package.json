		"@types/file-saver": "^2.0.5",
		"angular-material-fileupload": "^3.0.2",
		"angular2-jwt": "^0.2.3",
		"chart.js": "^3.9.1",
		"chartjs-adapter-moment": "^1.0.0",
		"chartjs-plugin-datalabels": "^0.7.0",
		"exceljs": "^4.3.0",
